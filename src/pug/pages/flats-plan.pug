						.building-filter__box
							.building-filter__list
								.building-filter__radio
									<input id="room1" type="radio" name="room" value="C" checked>
									<label for="room1">C</label>
								.building-filter__radio
									<input id="room2" type="radio" name="room" value="1">
									<label for="room2">1</label>
								.building-filter__radio
									<input id="room3" type="radio" name="room" value="2">
									<label for="room3">2</label>
								.building-filter__radio
									<input id="room4" type="radio" name="room" value="3+">
									<label for="room4">3+</label>
					.building-filter__col.building-filter__col--period
						.building-filter__header Срок сдачи
						.building-filter__box
							.building-filter__case
								.building-filter__wrp
									.building-filter__text Срок сдачи
									.building-filter__img
										<svg width="11" height="11" viewBox="0 0 11 11" fill="none" xmlns="http://www.w3.org/2000/svg">
											<path d="M1 5L5.24264 9.24264L9.48528 5" stroke="#1E202C" stroke-width="1.5"/>
										</svg>
								.building-filter__select
									.building-filter__unit
										<input id="period1" type="radio" name="period" value="2020" checked>
										<label for="period1" type="radio" title="2020">2020</label>
									.building-filter__unit
										<input id="period2" type="radio" name="period" value="2019">
										<label for="period2" title="2019">2019</label>
									.building-filter__unit
										<input id="period3" type="radio" name="period" value="2018">
										<label for="period3" title="2018">2018</label>
									.building-filter__unit
										<input id="period4" type="radio" name="period" value="2017">
										<label for="period4" title="2017">2017</label>
					.building-filter__col.building-filter__col--offer
						.building-filter__header Спецпредложения
						.building-filter__box
							.building-filter__case
								.building-filter__wrp
									.building-filter__text Спецпредложения
									.building-filter__img
										<svg width="11" height="11" viewBox="0 0 11 11" fill="none" xmlns="http://www.w3.org/2000/svg">
											<path d="M1 5L5.24264 9.24264L9.48528 5" stroke="#1E202C" stroke-width="1.5"/>
										</svg>
								.building-filter__select
									.building-filter__unit
										<input id="offer1" type="radio" name="offer" value="Все" checked>
										<label for="offer1">Все</label>
									.building-filter__unit
										<input id="offer2" type="radio" name="offer" value="Старт продаж">
										<label for="offer2">Старт продаж</label>
									.building-filter__unit
										<input id="offer3" type="radio" name="offer" value="Строительство">
										<label for="offer3">Строительство</label>
									.building-filter__unit
										<input id="offer4" type="radio" name="period" value="Завершенные">
										<label for="offer4">Завершенные</label>
					.building-filter__col.building-filter__col--cost
						<div id="polzunok"></div>

				.building-filter__row
				button.temp Отправить
