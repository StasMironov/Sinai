extends ../layout/main

block variables
	- var title = 'Главная страница'
	- var pageClass = 'index-page';

block content
	.flats-filter.mf-margin
		.flats-filter__container.b-container
			.flats-filter__title.title-1 Квартиры Завенягина 1/2
			.flats-filter__date 2 квартал 2021
			form(method="get").building-filter
				.building-filter__row
					.building-filter__col.building-filter__col--project
						.building-filter__header Проект
						.building-filter__box
							.building-filter__case
								.building-filter__wrp
									.building-filter__text Все
									.building-filter__img
										<svg width="11" height="11" viewBox="0 0 11 11" fill="none" xmlns="http://www.w3.org/2000/svg">
											<path d="M1 5L5.24264 9.24264L9.48528 5" stroke="#1E202C" stroke-width="1.5"/>
										</svg>
								.building-filter__select
									.building-filter__unit
										<input id="type1" type="radio" name="type" value="Завенягина 1/2" checked>
										<label for="type1" title="Завенягина 1">Завенягина 1/2</label>
									.building-filter__unit
										<input id="type2" type="radio" name="type" value="улица имени Газеты Правда, 79">
										<label for="type2" title="улица имени Газеты Правда, 79">улица имени Газеты Правда, 79</label>
									.building-filter__unit
										<input id="type3" type="radio" name="type" value="Завенягина 1/2" checked>
										<label for="type3" title="Завенягина 1">Завенягина 1/2</label>
									.building-filter__unit
										<input id="type4" type="radio" name="type" value="улица имени Газеты Правда, 79">
										<label for="type4" title="улица имени Газеты Правда, 79">улица имени Газеты Правда, 79</label>
					.building-filter__col.building-filter__col--room
